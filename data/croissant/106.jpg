			$('.footer_newsletter input').removeClass('redBorder');
			if($("#n_email").val()=='' || $("#n_email").val()=='Sign in our Newsletter')
			{
				$('.footer_newsletter .theInput').addClass('redBorder');
			}
			else
			{
				if(validateEmail($("#n_email").val()))
				{
					$.ajax({ 
						type:'POST',
						url:'/_newsletterSubscribe.php',
						data:'email='+$("#n_email").val(),
						cache:false,
						success: function(msg){
							if(msg=='-ok-')
								window.location.href='https://bindiusa.com/newsletter-subscribed/';
							else
								alert(msg);
						}
					});
				}
				else
					$('.footer_newsletter .theInput').addClass('redBorder');
			}
			return false;
		}
		
